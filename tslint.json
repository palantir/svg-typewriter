{
  "rules": {
    "class-name": false,
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [true, 2],
    "jsdoc-format": true,
    "label-position": true,
    "label-undefined": true,
    "max-line-length": [true, 140],
    "no-arg": true,
    "no-bitwise": true,
    "no-construct": true,
    "no-console": [true,
        "log",
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace",
        "warn"
    ],
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "no-unreachable": true,
    "no-unused-variable": false,
    "no-use-before-declare": true,
    "one-line": [true,
        "check-open-brace",
        "check-catch",
        "check-else"
    ],
    "quotemark": [true, "double"],
    "radix": true,
    "semicolon": true,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "triple-equals": [true, "allow-null-check"],
    "variable-name": false,
    "whitespace": ["check-branch", "check-decl", "check-type"]
  }
}
